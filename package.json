{
  "name": "@karasushin/eslint-config",
  "version": "1.5.0",
  "type": "module",
  "packageManager": "pnpm@8.6.7",
  "author": "Karasu Shin <karasu.shing@gmail.com>",
  "license": "MIT",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js",
    "types": "./types/index.d.ts"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "build:pkg": "shx cp package.json dist && json -I -f dist/package.json -e \"this.devDependencies=undefined; this.scripts=undefined; this.private=false;\"",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs && npm run build:pkg",
    "release": "bumpp package.json --commit --push --tag && npm run build && npm run build:pkg && npm publish --npmrc ./dist --access public"
  },
  "devDependencies": {
    "@karasushin/tsconfig": "^1.3.0",
    "@types/eslint-plugin-markdown": "^2.0.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^18.18.9",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "json": "^11.0.0",
    "rimraf": "^5.0.5",
    "shx": "^0.3.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@eslint/js": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.4.0",
    "vue-eslint-parser": "^9.3.2"
  },
  "peerDependencies": {
    "eslint": "^8.53.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarasuShin/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/KarasuShin/eslint-config/issues"
  },
  "homepage": "https://github.com/KarasuShin/eslint-config#readme"
}
