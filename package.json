{
  "name": "@karasushin/eslint-config",
  "version": "1.0.3",
  "type": "module",
  "packageManager": "pnpm@8.6.7",
  "author": "Karasu Shin <karasu.shing@gmail.com>",
  "license": "MIT",
  "exports": {
    "import": "./index.js"
  },
  "main": "./index.js",
  "module": "./index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "rimraf dist/* && shx cp src/* dist",
    "build:pkg": "shx cp package.json dist && json -I -f dist/package.json -e \"this.devDependencies=undefined; this.scripts=undefined; this.private=false;\"",
    "release": "bumpp package.json --commit --push --tag && npm run build && npm run build:pkg && npm publish --npmrc ./dist --access public"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "bumpp": "^9.1.1",
    "eslint": "^8.44.0",
    "eslint-define-config": "^1.21.0",
    "json": "^11.0.0",
    "rimraf": "^5.0.1",
    "shx": "^0.3.4",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@eslint/js": "^8.44.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "globals": "^13.20.0",
    "jsonc-eslint-parser": "^2.3.0",
    "vue-eslint-parser": "^9.3.1"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  }
}
